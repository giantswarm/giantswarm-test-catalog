{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "azureEnvironment": {
            "type": "string"
        },
        "azureTenantID": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "release": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                }
            }
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "logLevel": {
            "type": "string"
        },
        "metricsAddr": {
            "type": "string"
        },
        "metricsBackend": {
            "type": "string"
        },
        "mutatingWebhookAnnotations": {
            "type": "object"
        },
        "mutatingWebhookNamespaceSelector": {
            "type": "object"
        },
        "nodeSelector": {
            "type": "object",
            "properties": {
                "kubernetes.io/os": {
                    "type": "string"
                }
            }
        },
        "podAnnotations": {
            "type": "object"
        },
        "podDisruptionBudget": {
            "type": "object",
            "properties": {
                "minAvailable": {
                    "type": "integer"
                }
            }
        },
        "podLabels": {
            "type": "object"
        },
        "priorityClassName": {
            "type": "string"
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "port": {
                    "type": "integer"
                },
                "targetPort": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
