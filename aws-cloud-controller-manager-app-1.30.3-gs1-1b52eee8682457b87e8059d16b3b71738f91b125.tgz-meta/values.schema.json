                    "type": "integer"
                },
                "runAsUser": {
                    "type": "integer"
                },
                "seccompProfile": {
                    "type": "object",
                    "properties": {
                        "type": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "ports": {
            "type": "object",
            "properties": {
                "healthcheck": {
                    "type": "integer"
                }
            }
