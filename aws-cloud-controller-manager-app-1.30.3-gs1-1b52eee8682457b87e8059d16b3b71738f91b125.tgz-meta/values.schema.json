        "verticalPodAutoscaler": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "minAllowed": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        }
