{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "cluster": {
            "type": "object",
            "properties": {
                "proxy": {
                    "type": "object",
                    "properties": {
                        "http": {
                            "type": "null"
                        },
                        "https": {
                            "type": "null"
                        },
                        "noProxy": {
                            "type": "null"
                        }
                    }
                }
            }
        },
        "image": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "registry": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "k8sServiceHost": {
            "type": "string"
        },
        "k8sServicePort": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "podSecurityContext": {
            "type": "object",
            "properties": {
